<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tpcheck.tpri" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>压力管道安全管理</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>

	<icon src="res/logo.jpg" />

    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-targetSdkVersion" value="20" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-camera" source="npm" spec="2.1.1" />
    <plugin name="cordova-plugin-file" source="npm" spec="4.1.1" />
    <plugin name="cordova-plugin-file-transfer" source="npm" spec="1.5.0" />
    <plugin name="phonegap-plugin-barcodescanner" source="npm" spec="6.0.5">
        <param name="CAMERA_USAGE_DESCRIPTION" value="To scan barcodes." />
    </plugin>
    <plugin name="cordova-plugin-android-permissions" spec="~0.11.0" />
</widget>
